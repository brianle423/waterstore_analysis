ct(yanchor=\"bottom\", xanchor=\"left\"))\n    \n    fig.update_xaxes(\n        rangeslider_visible=True)\n    return fig","metadata":{"execution":{"iopub.status.busy":"2024-09-11T19:08:19.740599Z","iopub.execute_input":"2024-09-11T19:08:19.741066Z","iopub.status.idle":"2024-09-11T19:08:19.755965Z","shell.execute_reply.started":"2024-09-11T19:08:19.741013Z","shell.execute_reply":"2024-09-11T19:08:19.754777Z"},"trusted":true},"execution_count":11,"outputs":[]},{"cell_type":"code","source":"customer_compare(df)","metadata":{"execution":{"iopub.status.busy":"2024-09-11T19:08:19.757756Z","iopub.execute_input":"2024-09-11T19:08:19.758260Z","iopub.status.idle":"2024-09-11T19:08:19.828528Z","shell.execute_reply.started":"2024-09-11T19:08:19.758193Z","shell.execute_reply":"2024-09-11T19:08:19.827168Z"},"trusted":true},"execution_count":12,"outputs":[{"output_type":"display_data","data":{"application/vnd.plotly.v1+json":{"data":[{"customdata":["2017-09","2017-10","2017-11","2017-12","2018-01","2018-02","2018-03","2018-04","2018-05","2018-06","2018-07","2018-08"],"hovertemplate":"Customers: %{y}<br>Month: %{customdata}<extra></extra>","legendgroup":"revenue","marker":{"color":"lightblue"},"name":"Last Year","x":["September","October","November","December","January","February","March","April","May","June","July","August"],"y":[263,252,210,210,228,221,245,245,253,241,255,207],"yaxis":"y","type":"bar"},{"customdata":["2018-09","2018-10","2018-11","2018-12","2019-01","2019-02","2019-03","2019-04","2019-05","2019-06","2019-07","2019-08"],"hovertemplate":"Customers: %{y}<br>Month: %{customdata}<extra></extra>","legendgroup":"revenue","marker":{"color":"#3498db"},"name":"Current Year","x":["September","October","November","December","January","February","March","April","May","June","July","August"],"y":[265,248,226,217,256,204,267,220,235,249,240,254],"yaxis":"y","type":"bar"},{"hovertemplate":"YoY Growth: %{y:.1%}<extra></extra>","legendgroup":"trend","line":{"color":"orange","widt