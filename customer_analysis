(x=active.index.astype(str).to_list(), y=active['Name'], name='Customers',\n                        yaxis='y1', marker_color='lightblue',\n                         hovertemplate=\"<br>Customers: %{y}<extra></extra>\"))\n    fig.add_trace(go.Scatter(x=active.index.astype(str).to_list(), y=active['mom'], \n                        name='MoM Growth', visible='legendonly',  yaxis='y2',\n                             line=dict(width=2.5),\n                        hovertemplate='MoM Growth: %{y:.1%}<extra></extra>'))\n    fig.add_trace(go.Scatter(x=active.index.astype(str).to_list(), y=active['yoy'], \n                            name='YoY Growth', yaxis='y2',\n                            hovertemplate=\"YoY Growth: %{y:.1%}<extra></extra>\"))\n    \n    \n    fig.update_layout(\n            title='Monthly Active Customers', title_x=0.5,\n            yaxis_title='Customers',\n            yaxis2=dict(overlaying='y',side='right', tickformat = '%', title='Growth'), \n            legend=dict(yanchor=\"bottom\", xanchor=\"left\"))\n    \n    fig.update_xaxes(\n            rangeslider_visible=True,\n            rangeselector=dict(\n                buttons=list([\n                    dict(count=6, label=\"6m\", step=\"month\", stepmode=\"backward\"),\n                    dict(count=1, label=\"1y\", step=\"year\", stepmode=\"backward\"),\n                    dict(count=3, label=\"3y\", step=\"year\", stepmode=\"backward\"),\n                    dict(count=5, label=\"5y\", step=\"year\", stepmode=\"backward\"),\n                    dict(step=\"all\")\n                ])\n            )\n        )\n    \n    return fig","metadata":{"execution":{"iopub.status.busy":"2024-09-11T19:08:19.541029Z","iopub.execute_input":"2024-09-11T19:08:19.541531Z","iopub.status.idle":"2024-09-11T19:08:19.556366Z","shell.execute_reply.started":"2024-09-11T19:08:19.541444Z","shell.execute_reply":"2024-09-11T19:08:19.555049Z"},"trusted":true},"execution_count":8,"outputs":[]},{"cell_type":"code","source":"m