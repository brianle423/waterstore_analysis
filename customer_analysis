Date'].dt.to_period(period))\n        return df\n    \n    def get_retention(df, period='M'):\n        df = get_cohorts(df, period).groupby(['Acquisition_Cohort', 'Purchase_Cohort'])\\\n                                    .agg(Customers=('Name', 'nunique')) \\\n                                    .reset_index(drop=False)\n        df['Periods'] = (df.Purchase_Cohort - df.Acquisition_Cohort)\\\n        .apply(op.attrgetter('n'))\n        return df\n\n    def get_cohort_matrix(df, period='M', percentage=False):\n        df = get_retention(df, period).pivot_table(index = 'Acquisition_Cohort',\n                                                   columns = 'Periods',\n                                                   values = 'Customers')\n        if percentage:\n            df = df.divide(df.iloc[:,0], axis=0)\n        return df\n\n\n    df = get_cohort_matrix(df, period, percentage)\n    \n    periods = {'M':'Months', 'Q':'Quarters', 'Y':'Years'}\n    for i in periods.keys():\n        if i == period:\n            time = periods[i]\n\n    del df[0]\n    \n    z = df\n    y = df.index.astype(str).to_list()\n    x = df.columns\n\n    fig = go.Figure(data=go.Heatmap(\n            z=z,y=y,x=x,\n            colorscale='Blues'))\n    fig.update_layout(\n        title=f'Retention Rates Over {time}', title_x=0.5,\n        xaxis_title=f\"{time}\",\n        yaxis_title=\"Acquisition Cohort\")\n    \n    if reverse:\n        fig.update_yaxes(autorange = 'reversed')\n\n    fig.update_traces(hovertemplate=\"Membership Bought: %{y}\"\n                                    \"<br>Returned After: %{x}\" f' {time}'\n                                    \"<br>Retention Rate: %{z:.1%} <extra></extra>\",\n                     hoverlabel=dict(bgcolor=\"white\", font_size=16))\n    return fig ","metadata":{"execution":{"iopub.status.busy":"2024-09-11T19:08:19.229482Z","iopub.execute_input":"2024-09-11T19:08:19.229986Z","iopub.status.idle":"2024-09-11T19:08:19.247882Z","shell.execute_reply.started